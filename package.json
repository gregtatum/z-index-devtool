{
  "name": "z-index-devtool",
  "version": "1.0.0",
  "description": "A UCOSP project for building a Z-Index Devtool",
  "main": "index.js",
  "scripts": {
    "start:sandbox": "budo src/stacking-context/load-example.js --serve bundle.js --dir docs/sandbox",
    "build:sandbox": "browserify src/stacking-context/load-example.js > docs/sandbox/bundle.js",
