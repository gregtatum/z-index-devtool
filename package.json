  "bugs": {
    "url": "https://github.com/gregtatum/z-index-devtool/issues"
  },
  "homepage": "https://github.com/gregtatum/z-index-devtool#readme",
  "devDependencies": {
    "@tatumcreative/get": "1.0.0",
    "browserify": "^13.1.0",
    "budo": "^9.2.0",
    "react": "15.3.2",
    "react-dom": "15.3.2"
  }
}
